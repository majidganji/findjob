<div class="row">
    <div class="col-sm-12">
        <ol class="breadcrumb">
            <li><a href="/dashboard">صفحه اصلی</a></li>
            <li class="active"><%= title %></li>
        </ol>
    </div>
</div>
<div class="row">
    <div class="col-sm-12">
        <a href="/log/deleteAll" class="btn btn-danger">حذف همه</a>
    </div>
</div>
<p></p>
<div class="row">
    <div class="col-sm-12">
        <table class="table table-responsive table-bordered">
            <thead>
            <tr>
                <th class="col-sm-1">#</th>
                <th>نام کاربری</th>
                <th>زمان ورود</th>
                <th>زمان خروج</th>
                <th class="col-sm-1"><span class="fa fa-gear"></span></th>
            </tr>
            </thead>
            <tbody>
            <% var index = 1; %>
            <% _.each(models, function(model){ %>
            <tr>
                <td><%= index++ %></td>
                <td><%= model.username %></td>
                <td><%= moment(model.createdAt, 'fa').format("dddd, MMMM Do YYYY, h:mm:ss a") %></td>
                <td><%= model.logout ? moment(new Date(model.logout), 'fa').format("dddd, MMMM Do YYYY, h:mm:ss a") : '-----' %></td>

                <td>
                    <a href="/log/<%= model.id %>/delete" class="text-danger" onclick="return confirm('آیا مطمئن هستید؟')"><span class="fa fa-trash"></span></a>
                </td>
            </tr>
            <% }); %>
            </tbody>
        </table>
    </div>
</div>
<% if(count > 1){ %>
<div class="row">
    <div class="col-sm-12">
        <nav aria-label="Page navigation">
            <ul class="pagination">
                <% for(var i = 1; i <= count; i++){ %>
                <% if(pageid === i){ %>
                <li class="active"><a href="#"><%= i %></a></li>
                <% }else{ %>
                <li><a href="/log?page=<%= i %>"><%= i %></a></li>
                <% } %>
                <% } %>
            </ul>
        </nav>
    </div>
</div>
<% } %>